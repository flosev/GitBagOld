<?xml version="1.0" encoding="UTF-8"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd "
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans">

    <bean id="themeDao" class="net.golovach.quiz.dao.impl.ThemeDaoMock" scope="prototype"/>
    <bean id="quizDao" class="net.golovach.quiz.dao.impl.QuizDaoMock" scope="prototype"/>
    <bean id="questionDao" class="net.golovach.quiz.dao.impl.QuestionDaoMock" scope="prototype">
          <property name="object" value="quizDao"/>
    </bean>
</beans>